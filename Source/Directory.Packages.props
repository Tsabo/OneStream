<Project>
  <Import Project=".\Dependencies.props" />

  <PropertyGroup>
    <AspireVersion>8.2.0</AspireVersion>
    <MicrosoftExtensionsVersion>8.6.0</MicrosoftExtensionsVersion>
    <OpenTelemetry>1.9.0</OpenTelemetry>
  </PropertyGroup>

  <ItemGroup Label="Aspire Package References">
    <PackageVersion Include="Aspire.Azure.AI.OpenAI" Version="8.0.2-preview.1.24326.4" />
    <PackageVersion Include="Aspire.Azure.Storage.Blobs" Version="$(AspireVersion)" />
